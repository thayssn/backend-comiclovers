{
  "editor.formatOnSave": false,
  "prettier.eslintIntegration": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "typescript",
      "autoFix": true
    }
  ],
  "editor.maxTokenizationLineLength": 80000,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
